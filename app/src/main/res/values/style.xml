<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="FlatButton" parent="Theme.AppCompat.Light">
        <item name="android:textColor">@color/white</item>
        <item name="android:background">#34304B</item>
        <item name="colorControlHighlight">#34304B</item>
    </style>

    <style name="PrimaryButtonStyle" parent="@style/Widget.MaterialComponents.Button">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">#34304B</item>
        <item name="textAllCaps">false</item>
        <item name="android:textSize">12sp</item>
    </style>
</resources>